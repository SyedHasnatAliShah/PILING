for t in range(int(input())):
    
    lst = [] 
    n = int(input()) 
    for i in range(0, n): 
        ele = int(input()) 
        lst.append(ele) 
    print (lst)
    l = len(lst)
    i = 0
    while i < l - 1 and lst[i] >= lst[i+1]:
        i += 1
    while i < l - 1 and lst[i] <= lst[i+1]:
        i += 1
    if i == l - 1:
        print("yes")
    else: 
        print("No")
